  "resolutions": {
    "web3": "^1.7.5",
    "@ethersproject/providers": "5.7.2",
    "rollup": "3.20.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7"
  },
  "scripts": {
    "start": "vite",
    "build": "echo 'DO NOT disable validation on build, fix underlying issue' && yarn validate && tsc && vite build",
    "build-only": "tsc && vite build",
    "preview": "vite preview",
    "validate": "ts-node ./scripts/validatePools.ts",
    "creationdate": "ts-node ./scripts/getPoolCreationTimestamp.ts",
    "launchpool": "ts-node ./scripts/launchpool.ts",
    "vault": "ts-node ./scripts/addVault.ts",
    "migratePlatform": "ts-node ./scripts/migratePlatform.ts",
    "checkOneInchLiquidity": "ts-node -r dotenv/config ./scripts/checkOneInchLiquidity.ts",
    "setVaultUrls": "ts-node -r dotenv/config ./scripts/setVaultUrls.ts && prettier --write ./src/config/vault/*.json",
    "addAmmsToVaults": "ts-node -r dotenv/config ./scripts/addAmmsToVaults.ts && prettier --write ./src/config/vault/*.json",
    "prepare": "husky install",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "ts-node": "ts-node",
    "analyze-bundle": "tsc && cross-env ANALYZE_BUNDLE=true vite build",
    "analyze-sources": "madge --extensions js,jsx,ts,tsx ./src --image madge.svg",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/node": "^16.18.23",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "madge": "^5.0.1",
    "p-queue": "^6.6.2",
    "prettier": "^2.8.8",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "ts-node": "^10.8.2",
    "typescript": "4.9.3",
    "vite": "^4.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  }
}
