  "resolutions": {
    "web3": "^1.10.4",
    "rollup": "3.27.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0"
  },
  "scripts": {
    "prepare": "husky",
    "start": "vite",
    "build": "echo 'DO NOT disable validation on build, fix underlying issue' && yarn validate && tsc && vite build",
    "build-only": "tsc && vite build",
    "preview": "vite preview",
    "validate": "ts-node ./scripts/validatePools.ts",
    "creationdate": "ts-node ./scripts/getPoolCreationTimestamp.ts",
    "launchpool": "ts-node ./scripts/launchpool.ts",
    "vault": "ts-node ./scripts/addVault.ts",
    "clm": "ts-node ./scripts/addCLM.ts",
    "pause": "ts-node ./scripts/pausePlatform.ts",
    "setVaultUrls": "ts-node -r dotenv/config ./scripts/setVaultUrls.ts && prettier --write ./src/config/vault/*.json",
    "setVaultStatus": "ts-node -r dotenv/config ./scripts/setVaultStatus.ts",
    "addClmRewardPools": "ts-node -r dotenv/config ./scripts/addClmRewardPools.ts",
    "updateRewardPoolRewards": "ts-node -r dotenv/config ./scripts/updateRewardPoolRewards.ts",
    "addCurveZap": "ts-node -r dotenv/config ./scripts/addCurveZap.ts",
    "addBalancerZap": "ts-node -r dotenv/config ./scripts/addBalancerZap.ts",
    "checkVaultTokenDecimals": "ts-node -r dotenv/config ./scripts/checkVaultTokenDecimals.ts && prettier --write ./src/config/vault/*.json",
    "checkZapAddresses": "ts-node -r dotenv/config ./scripts/checkZapAddresses.ts",
    "makeExcludeConfig": "ts-node -r dotenv/config ./scripts/makeExcludeConfig.ts",
    "updatePlatforms": "ts-node -r dotenv/config ./scripts/updatePlatforms.ts",
    "updateNativeToWNativeVaultNames": "ts-node ./scripts/updateVaultWithNativeName.ts",
    "cli": "npm exec --yes @beefyfinance/beefy-app-cli@latest --",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "ts-node": "ts-node",
    "analyze-bundle": "tsc && cross-env ANALYZE_BUNDLE=true vite build",
    "analyze-sources": "madge --extensions js,jsx,ts,tsx ./src --image madge.svg",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}",
    "lint": "eslint --no-config-lookup -c eslint.config.mjs src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@eslint/js": "^9.14.0",
    "@types/command-line-usage": "^5.0.4",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "madge": "^5.0.1",
    "prettier": "^2.8.8",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-command-line-args": "^2.5.1",
    "ts-node": "^10.8.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^4.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "packageManager": "yarn@1.22.22"
}
