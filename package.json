{
  "name": "beefy-pro",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@clover-network/clover-connector": "^1.0.0",
    "@download/blockies": "^1.0.3",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/solidity": "^5.4.0",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/system": "^4.11.3",
    "@reduxjs/toolkit": "^1.7.1",
    "@transak/transak-sdk": "^1.0.31",
    "@walletconnect/web3-provider": "^1.4.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "blockchain-addressbook": "^0.21.3",
    "chokidar": "^3.5.2",
    "clsx": "^1.1.1",
    "deficonnect": "^1.1.5",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-animate-height": "^2.0.23",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.11.1",
    "react-indiana-drag-scroll": "^2.1.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-virtualized": "^9.22.3",
    "recharts": "^2.0.7",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "siema": "^1.5.1",
    "walletlink": "^2.3.0",
    "web-vitals": "^0.2.4",
    "web3": "^1.7.0",
    "web3modal": "^1.9.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=UTC react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "ts-node": "ts-node",
    "analyze-bundles": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "analyze-sources": "madge --extensions js,jsx,ts,tsx ./src --image madge.svg",
    "sync": "node --loader ts-node/esm src/scripts/migrateV1objects.mjs && yarn upgrade blockchain-addressbook --latest",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-restricted-syntax": [
        "error",
        {
          "selector": "ExportDefaultDeclaration",
          "message": "Prefer named exports"
        }
      ],
      "no-mixed-operators": "off",
      "react/jsx-no-target-blank": [
        "error",
        {
          "allowReferrer": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "canvas": "^2.7.0",
    "eth-multicall": "^1.3.13",
    "husky": ">=6",
    "lint-staged": ">=10",
    "madge": "^5.0.1",
    "prettier": "^2.3.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "^worker-loader!.+$": "<rootDir>/src/features/data/utils/mock-worker-loader.js"
    }
  }
}
