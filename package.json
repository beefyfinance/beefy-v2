{
  "name": "beefy-v2",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.18.2 || ^22.14.0"
  },
  "packageManager": "npm@11.1.0",
  "scripts": {
    "prepare": "husky",
    "start": "vite-node ./scripts/cli.ts dev",
    "build": "vite-node ./scripts/cli.ts build",
    "build:skip-validate": "vite-node ./scripts/cli.ts build --skip-validate",
    "preview": "vite preview",
    "clean": "node ./scripts/clean.js",
    "validate": "vite-node ./scripts/validatePools.ts",
    "creationdate": "vite-node ./scripts/getPoolCreationTimestamp.ts",
    "launchpool": "vite-node ./scripts/launchpool.ts",
    "vault": "vite-node ./scripts/addVault.ts",
    "clm": "vite-node ./scripts/addCLM.ts",
    "pause": "vite-node ./scripts/pausePlatform.ts",
    "setVaultUrls": "vite-node ./scripts/setVaultUrls.ts",
    "setVaultStatus": "vite-node ./scripts/setVaultStatus.ts",
    "addClmRewardPools": "vite-node ./scripts/addClmRewardPools.ts",
    "addCurveZap": "vite-node ./scripts/addCurveZap.ts",
    "addBalancerZap": "vite-node ./scripts/addBalancerZap.ts",
    "addBeraSwapZap": "vite-node ./scripts/addBeraSwapZap.ts",
    "checkVaultTokenDecimals": "vite-node ./scripts/checkVaultTokenDecimals.ts",
    "checkZapAddresses": "vite-node ./scripts/checkZapAddresses.ts",
    "makeExcludeConfig": "vite-node ./scripts/makeExcludeConfig.ts",
    "updatePlatforms": "vite-node ./scripts/updatePlatforms.ts",
    "tsc": "tsc --project tsconfig.app.json",
    "tsc:watch": "tsc --project tsconfig.app.json --watch",
    "tsc:scripts": "tsc --project tsconfig.scripts.json",
    "tsc:scripts:watch": "tsc --project tsconfig.scripts.json --watch",
    "analyze:bundle": "vite-node ./scripts/cli.ts analyze:bundle",
    "analyze:source": "madge --extensions ts,tsx --ts-config tsconfig.app.json ./src --image ./analyze-source.svg",
    "analyze:source:unused": "echo 'DOES NOT ACCOUNT FOR DYNAMIC IMPORTS / VITE GLOBS' && madge --extensions ts,tsx --ts-config tsconfig.app.json ./src --orphans",
    "analyze:source:circular": "madge --extensions ts,tsx --ts-config tsconfig.app.json ./src --circular",
    "prettier": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}",
    "lint": "eslint -c eslint.config.mjs --no-config-lookup --cache --cache-location .cache/eslint/ src",
    "lint:fix": "eslint -c eslint.config.mjs --no-config-lookup --cache --cache-location .cache/eslint/ --fix src",
    "lint:scripts": "eslint -c eslint.config.mjs --no-config-lookup --cache --cache-location .cache/eslint/ scripts",
    "packages:update": "node ./scripts/updatePackages.js"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^4.3.2",
    "@deficonnect/web3-connector": "^2.0.3",
    "@floating-ui/react": "^0.27.6",
    "@reduxjs/toolkit": "1.9.7",
    "@web3-onboard/coinbase": "^2.4.2",
    "@web3-onboard/core": "^2.24.0",
    "@web3-onboard/injected-wallets": "^2.11.3",
    "@web3-onboard/metamask": "^2.2.1",
    "@web3-onboard/trust": "^2.1.2",
    "@web3-onboard/walletconnect": "^2.6.2",
    "bignumber.js": "^9.2.0",
    "blockchain-addressbook": "^0.52.11",
    "date-fns": "^4.1.0",
    "escape-string-regexp": "^5.0.0",
    "i18next": "^20.6.1",
    "lodash-es": "^4.17.21",
    "p-queue": "^8.1.0",
    "re-reselect": "^5.1.0",
    "react": "^18.3.1",
    "react-animate-height": "^2.1.2",
    "react-content-loader": "^6.2.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^11.18.6",
    "react-redux": "8.1.3",
    "react-resize-detector": "^7.1.2",
    "react-router": "^7.5.0",
    "react-virtuoso": "^4.12.6",
    "recharts": "^2.15.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "viem": "^2.26.0"
  },
  "overrides": {
    "@coinbase/wallet-sdk": "^4.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "viem": "^2.26.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "rollup": "^4.39.0",
    "esbuild": "^0.25.2",
    "bnc-sdk": {
      "crypto-es": "^2.1.0"
    },
    "@web3-onboard/core": {
      "nanoid@4.0.2": "^5.1.2",
      "svelte": "^4.2.19"
    }
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@pandacss/dev": "^0.53.3",
    "@types/babel__core": "^7.20.5",
    "@types/command-line-usage": "^5.0.4",
    "@types/cross-spawn": "^6.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-spawn": "^7.0.6",
    "csv-parser": "^3.2.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-barrel-files": "^1.2.1",
    "eslint-plugin-react-dom": "^1.40.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react-x": "^1.40.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "madge": "^8.0.0",
    "prettier": "^3.5.3",
    "rollup": "^4.39.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-command-line-args": "^2.5.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-node": "^3.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
