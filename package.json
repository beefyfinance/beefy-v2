{
  "name": "beefy-pro",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.0.10",
    "@deficonnect/web3-connector": "^2.0.3",
    "@download/blockies": "^1.0.3",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/solidity": "^5.4.0",
    "@floating-ui/react-dom": "^0.7.0",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/system": "^4.11.3",
    "@reduxjs/toolkit": "1.9.3",
    "@siddomains/sidjs": "^0.1.8",
    "@walletconnect/web3-provider": "^1.4.1",
    "@web3-onboard/coinbase": "^2.0.10",
    "@web3-onboard/core": "^2.8.3",
    "@web3-onboard/gas": "^2.1.4",
    "@web3-onboard/injected-wallets": "^2.2.2",
    "@web3-onboard/transaction-preview": "^2.0.4",
    "@web3-onboard/walletconnect": "^2.1.2",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "blockchain-addressbook": "^0.41.24",
    "clsx": "^1.1.1",
    "date-fns": "^2.29.3",
    "eip-712": "^1.0.0",
    "escape-string-regexp": "^5.0.0",
    "eth-multicall": "^1.3.13",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "re-reselect": "^4.0.0",
    "react": "18.2.0",
    "react-animate-height": "^2.0.23",
    "react-content-loader": "^6.0.3",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^11.11.1",
    "react-intersection-observer": "^9.1.0",
    "react-redux": "8.0.5",
    "react-resize-detector": "^7.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.1.15",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "web3": "^1.7.5"
  },
  "resolutions": {
    "web3": "^1.7.5"
  },
  "scripts": {
    "start": "vite",
    "build": "echo 'DO NOT disable validation on build, fix underlying issue' && yarn validate && tsc && vite build",
    "build-only": "tsc && vite build",
    "preview": "vite preview",
    "validate": "ts-node ./scripts/validatePools.ts",
    "creationdate": "ts-node ./scripts/getPoolCreationTimestamp.ts",
    "launchpool": "ts-node ./scripts/launchpool.ts",
    "vault": "ts-node ./scripts/addVault.ts",
    "migratePlatform": "ts-node ./scripts/migratePlatform.ts",
    "checkOneInchLiquidity": "ts-node -r dotenv/config ./scripts/checkOneInchLiquidity.ts",
    "addAmmsToVaults": "ts-node -r dotenv/config ./scripts/addAmmsToVaults.ts && prettier --write ./src/config/vault/*.json",
    "prepare": "husky install",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "ts-node": "ts-node",
    "analyze-bundles": "tsc && vite build && source-map-explorer 'build/static/js/*.js'",
    "analyze-sources": "madge --extensions js,jsx,ts,tsx ./src --image madge.svg",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-restricted-syntax": [
        "error",
        {
          "selector": "ExportDefaultDeclaration",
          "message": "Prefer named exports"
        }
      ],
      "no-mixed-operators": "off",
      "react/jsx-no-target-blank": [
        "error",
        {
          "allowReferrer": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^3.1.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "madge": "^5.0.1",
    "p-queue": "^6.6.2",
    "prettier": "^2.3.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.8.2",
    "typescript": "4.9.3",
    "vite": "^4.1.4",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "^worker-loader!.+$": "<rootDir>/src/features/data/utils/mock-worker-loader.js"
    }
  }
}
